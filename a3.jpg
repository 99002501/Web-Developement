	function showMoreLinks(id, blockId) {
		var blockDiv = document.getElementById(blockId);
		var elementList = blockDiv.getElementsByClassName(id + 'Overflow');
		var elementsSize = elementList.length;
		for(var idx = 0; idx<elementsSize; idx++) {
			var element = elementList.item(idx);
			element.classList.remove("hidden");
		}
		var showMoreButton = document.getElementById(id + 'MoreLink');
		showMoreButton.classList.add("hidden");
		var showLessButton = document.getElementById(id + 'LessLink');
		showLessButton.classList.remove("hidden");
	}
	function showLessLinks(id, blockId) {
		var blockDiv = document.getElementById(blockId);
		var elementList = blockDiv.getElementsByClassName(id + 'Overflow');
		var elementsSize = elementList.length;
		for(var idx = 0; idx<elementsSize; idx++) {
			var element = elementList.item(idx);
			element.classList.add("hidden");
		}
		var showMoreButton = document.getElementById(id + 'MoreLink');
		showMoreButton.classList.remove("hidden");
		var showLessButton = document.getElementById(id + 'LessLink');
		showLessButton.classList.add("hidden");
	}
	
	var showFAQ =  document.getElementById("showFaq");
	if(showFAQ){
		showFAQ.onclick = function () {
			faqBlockDisplay();
		};
	}
		
	var closeFAQ = document.getElementById("closeFaq");
	if(closeFAQ){
		closeFAQ.onclick = function () {
			faqBlockDisplay();
		};
	}
 
	function faqBlockDisplay() {
			var element = document.getElementById("allFaq");
		if (element.style.display == 'block') {
			element.style.display = 'none';
		} else {
			element.style.display = 'block';
		}
		}
	function showMoreLinksInfoBlock(id, blockId, visibleBlockId) {
		var visibleBlockDiv = document.getElementById(visibleBlockId);
		visibleBlockDiv.classList.add("hidden");
		
		showMoreLinks(id, blockId);
	}
	function showLessLinksInfoBlock(id, blockId, visibleBlockId) {
		var visibleBlockDiv = document.getElementById(visibleBlockId);
		visibleBlockDiv.classList.remove("hidden");
		
		showLessLinks(id, blockId);
	}
</script>
